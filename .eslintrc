{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "parser": "@typescript-eslint/parser"
  },
  "parser": "vue-eslint-parser",
  "extends": [
    "plugin:vue/vue3-recommended",
    "eslint:recommended",
    "prettier",
    "plugin:prettier/recommended"
  ],

  "plugins": ["vue", "prettier", "@typescript-eslint"],
  "rules": {
    "prefer-const": "error",
    "vue/multi-word-component-names": "off"
  }
}
